---
layout: 'default'
---

<ul class="entry-list">
<% for post in @getFilesAtPath('posts',[date: -1]).toJSON()[1..20]: %>
  <li>
    <a href="<%= post.url %>">
      <span class="entry-list__date"><%= post.humanDate %></span>
      <span class="entry-list__title"><%= post.title %></span>
    </a>
  </li>
<% end %>
</ul>