<!DOCTYPE html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <!-- Site Properities -->
  <title><%= @getPreparedTitle() %></title>
  <meta name="description" content="<%= @getPreparedDescription() %>" />
  <meta name="keywords" content="<%= @getPreparedKeywords() %>" />
  <!-- ogp -->
    <meta property="og:title" content="<%= @getPreparedTitle() %>">
    <meta property="og:url" content="<%= @site.url %><%= @document.url %>">
    <meta property="og:image" content="">
    <meta property="og:description" content="<%= @getPreparedDescription() %>"> 
    <meta property="og:site_name" content="emotokahori">  
  <!-- DocPad Styles + Our Own -->
  <%- @getBlock('styles').add(@getGruntedStyles()).toHTML() %>
  <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
  <link href='http://fonts.googleapis.com/css?family=Averia+Sans+Libre:700' rel='stylesheet' type='text/css'>
  <!-- google analytics -->
</head>
<body>
  <div class="l_wrapper">
    <header class="l_header">
    	<div class="m_inner">
        <h1 class="header-title"><a href="/">emotokahori</a></h1>
        <ul class="m_list__line m_text-align__right">
					<li><a href="https://twitter.com/tomo_e" target="_blank">Twitter</a></li>
					<li><a href="https://github.com/tomo-e" target="_blank">Github</a></li>
				</ul>
      </div>
    </header>
    <div class="l_content">
      <div class="m_inner">
        <%- @content %>
      </div>
    </div><!-- .l_content -->
    <footer class="l_footer">
      <div class="m_inner">
				<dl>
					<dt>tomo_e</dt>
					<dd>Front-end Engineer / Illustrator</dd>
          <dd>イラストのお仕事もゆるふわで受けてます〜♫</dd>
				</dl>
			</div>
    </footer>
  </div><!-- .l_wrapper -->
  <!-- DocPad Scripts + Our Own -->
  <%- @getBlock('scripts').add(@getGruntedScripts()).toHTML() %>
</body>
</html>